     *企业裁判文书详情
     * */
    app.controller('businessDocumentCheck', ['$scope', function ($scope) {
        //没有接口 自己模拟的数据
        $scope.mockData =
            {
                code: "18860469551",
                time: "2011-1-1"
            };
        //数据初始化
        $scope.info =
            {
                code: "18860469551"
            };

        //结果与样例的切换初始化
        $scope.resultSwitch = false;

        //重置数据
        $scope.resetInfo = function () {
            $scope.info.code = '';
            $scope.resultSwitch = false;
        };


        //提交
        $scope.submitInfo = function (myForm) {

            //提交空表单时提示格式错误
            if (myForm.code) {
                myForm.code.$dirty = true;

            }
            if (!myForm.$valid) {
                $scope.resultSwitch = false;
                return
            }

            $scope.resultSwitch = false;

            if($scope.info.code === $scope.mockData.code){
                console.log('ddd');
                $scope.resultSwitch = true;
            }


        };

        $scope.test=[
            {
                sheanType:"aa",
                sheanCla:"ee",
                sheanClass:"qq"
            },
            {
                sheanType:"11",
                sheanCla:"66",
                sheanClass:"88"
            }
        ]

    }]);
</script>


